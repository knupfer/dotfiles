"1.1

loadplugins '\.(js|penta)$'
group user
map -count -modes=n <C-x><Left> -builtin <count><C-p>
map -count -modes=n <C-x><Right> -builtin <count><C-n>
map -count -modes=n <C-x>k -builtin <count>d
map -count -modes=n <S-Space> -builtin <count>f
map -count -modes=n <Space> -builtin <count>F
map -count -modes=b <A-x> -builtin :
map -count -modes=b <C-g> -builtin <Esc>
map -modes=b <C-x><C-c> -ex :exit
map -modes=b <C-h> -ex :help
set defsearch=duckduckgo
set guioptions=bCs
set hintkeys=tarenish
set showtabline=multitab

" vim: set ft=pentadactyl:
